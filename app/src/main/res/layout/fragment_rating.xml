<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimary"
    tools:context="com.toilet.toilet_times.fragments.WhereAreYou">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="80dp"
        android:text="Rate this bathroom"
        android:textColor="@color/colorAccent"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <!-- Left arrow -->
    <ImageButton
        android:id="@+id/left_arrow_rating"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginTop="234dp"
        android:background="@null"
        android:src="@drawable/left_arrow"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <!-- Right arrow -->
    <ImageButton
        android:id="@+id/right_arrow_rating"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginTop="234dp"
        android:background="@null"
        android:src="@drawable/right_arrow"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="98dp"
        android:layout_height="98dp"
        android:layout_marginEnd="20dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="12dp"
        android:src="@drawable/roll_art"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.526"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <ImageView
        android:id="@+id/squareFive"
        android:layout_width="75dp"
        android:layout_height="68dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="180dp"
        android:src="@drawable/square"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/squareFour"
        android:layout_width="76dp"
        android:layout_height="68dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="240dp"
        android:src="@drawable/square"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/squareThree"
        android:layout_width="75dp"
        android:layout_height="68dp"
        android:src="@drawable/square"
        android:layout_marginTop="300dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="29dp"/>

    <ImageView
        android:id="@+id/squareTwo"
        android:layout_width="75dp"
        android:layout_height="68dp"
        android:layout_marginTop="360dp"
        android:src="@drawable/square"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="29dp"/>

    <ImageView
        android:id="@+id/squareOne"
        android:layout_width="75dp"
        android:layout_height="68dp"
        android:src="@drawable/square"
        android:layout_marginTop="420dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="29dp"/>

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="1"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/squareOne"
        app:layout_constraintEnd_toEndOf="@+id/squareOne"
        app:layout_constraintStart_toStartOf="@+id/squareOne"
        app:layout_constraintTop_toTopOf="@+id/squareOne" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="2"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/squareTwo"
        app:layout_constraintEnd_toEndOf="@+id/squareTwo"
        app:layout_constraintStart_toStartOf="@+id/squareTwo"
        app:layout_constraintTop_toTopOf="@+id/squareTwo" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="3"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/squareThree"
        app:layout_constraintEnd_toEndOf="@+id/squareThree"
        app:layout_constraintStart_toStartOf="@+id/squareThree"
        app:layout_constraintTop_toTopOf="@+id/squareThree" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="4"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/squareFour"
        app:layout_constraintEnd_toEndOf="@+id/squareFour"
        app:layout_constraintStart_toStartOf="@+id/squareFour"
        app:layout_constraintTop_toTopOf="@+id/squareFour" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="5"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/squareFive"
        app:layout_constraintEnd_toEndOf="@+id/squareFive"
        app:layout_constraintStart_toStartOf="@+id/squareFive"
        app:layout_constraintTop_toTopOf="@+id/squareFive" />

</android.support.constraint.ConstraintLayout>